    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@farmfe/cli": "^1.0.5",
    "@farmfe/core": "^1.7.11",
    "@farmfe/js-plugin-vue": "^3.13.0",
    "@types/croppie": "^2.6.4",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
