    "lint-staged": "^15.5.2",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "terser": "^5.46.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.12"
